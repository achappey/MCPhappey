{
  "resources": [
    {
      "uri": "https://graph.microsoft.com/beta/users?$orderby=displayName asc&$top=100&$select=id,displayName,userPrincipalName,mail,preferredLanguage,employeeId,hireDate,mobilePhone,otherMails,givenName,surname,userType,aboutMe,jobTitle,companyName,department,officeLocation,accountEnabled,createdDateTime",
      "name": "all-users",
      "title": "All users (top 100)",
      "description": "List of users with rich profile fields.",
      "mimeType": "application/json",
      "annotations": {
        "audience": [
          "assistant"
        ]
      }
    },
    {
      "uri": "https://graph.microsoft.com/beta/users?$filter=manager eq null&$select=id,displayName,userPrincipalName,department,jobTitle",
      "name": "users-without-manager",
      "title": "Users without manager",
      "description": "Quick check for missing manager relationships.",
      "mimeType": "application/json",
      "annotations": {
        "audience": [
          "assistant"
        ]
      }
    },
    {
      "uri": "https://graph.microsoft.com/beta/users?$filter=accountEnabled eq false&$orderby=displayName asc&$top=100&$select=id,displayName,userPrincipalName,mail,jobTitle,department,accountEnabled,createdDateTime",
      "name": "disabled-users",
      "title": "Disabled users (top 100)",
      "description": "Quick list of disabled accounts.",
      "mimeType": "application/json",
      "annotations": {
        "audience": [
          "assistant"
        ]
      }
    }
  ]
}